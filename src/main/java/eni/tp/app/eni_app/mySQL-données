//Dans MySQL :
//Pour créer la table "film":
USE db_film;

CREATE TABLE film (
	id int NOT NULL AUTO_INCREMENT,
    title VARCHAR(255),
    note int,
    year int,
    duration int,
    synopsis TEXT,
    genre VARCHAR(255),
    image TEXT,
    PRIMARY KEY(id)
) Engine=InnoDB;



